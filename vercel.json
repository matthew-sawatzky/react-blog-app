{
  "builds": [
    {
      "src": "dummy-backend/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "blog-application/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "blog-application/$1"
    }
  ],
  "build": {
    "command": "cd dummy-backend && npm install && npm start && cd ../blog-application && npm install && npm run dev"
  }
}
